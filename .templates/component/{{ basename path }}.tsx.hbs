import { ReactElement } from "react";
import { useTranslation } from "react-i18next";

type Props = {
  data?: string;
};

export const {{ basename path }} = ({ data }: Props): ReactElement => {
  const { t } = useTranslation("common", { keyPrefix: "{{ basename path }}" });
  return (
    <div>
      <p>{t("{{ basename path }}")}</p>
      <div>{data}</div>
    </div>
  );
};
